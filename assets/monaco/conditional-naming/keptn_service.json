{
  "type": "SERVICE",
  "nameFormat": "{ProcessGroup:KubernetesContainerName}",
  "displayName": "{{ .name }}",
  "enabled": true,
  "rules": [
    {
      "key": {
        "attribute": "PROCESS_GROUP_PREDEFINED_METADATA",
        "dynamicKey": "KUBERNETES_NAMESPACE",
        "type": "PROCESS_PREDEFINED_METADATA_KEY"
      },
      "comparisonInfo": {
        "type": "STRING",
        "operator": "EQUALS",
        "value": "{{ .serviceName }}",
        "negate": false,
        "caseSensitive": true
      }
    },
    {
      "key": {
        "attribute": "SERVICE_DETECTED_NAME",
        "type": "STATIC"
      },
      "comparisonInfo": {
        "type": "STRING",
        "operator": "EQUALS",
        "value": "default web request",
        "negate": false,
        "caseSensitive": true
      }
    }
  ]
}